<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox onDragDropped="#handleDragDropped" onDragOver="#handleDragOver" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="puzzlewithtimer.controller.FXMLController">
    <children>
        <MenuBar VBox.vgrow="NEVER">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" text="New" />
                        <MenuItem mnemonicParsing="false" text="Openâ€¦" />
                        <Menu mnemonicParsing="false" text="Open Recent" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" onAction="#handleQuit" text="Quit" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Undo" />
                        <MenuItem mnemonicParsing="false" text="Redo" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About PuzzleWithTimer" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <AnchorPane fx:id="mainPane" maxHeight="-1.0" maxWidth="-1.0" prefHeight="300.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <Button fx:id="startButton" layoutX="15.0" layoutY="330.0" mnemonicParsing="false" onAction="#handleStart" text="Start" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="15.0" />
                <Button fx:id="selectButton" layoutX="90.0" layoutY="330.0" mnemonicParsing="false" onAction="#handleSelect" text="Select" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="85.0" />
                <Button fx:id="resultButton" layoutX="570.0" layoutY="330.0" mnemonicParsing="false" onAction="#handleResult" text="Result" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="15.0" /><Button layoutX="428.0" layoutY="320.0" mnemonicParsing="false" onAction="#handleSelect" text="Hint" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="85.0" />
            </children>
        </AnchorPane>
    </children>
</VBox>
